# Install Script

build-install:
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_FLASH_START"="%CONFIG_SPX_FEATURE_GLOBAL_FLASH_START" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_FLASH_SIZE=%CONFIG_SPX_FEATURE_GLOBAL_FLASH_SIZE" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_ERASE_BLOCK_SIZE=%CONFIG_SPX_FEATURE_GLOBAL_ERASE_BLOCK_SIZE" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_FLASH_BANKS=%CONFIG_SPX_FEATURE_GLOBAL_FLASH_BANKS" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_USED_FLASH_START=%CONFIG_SPX_FEATURE_GLOBAL_USED_FLASH_START" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_USED_FLASH_SIZE=%CONFIG_SPX_FEATURE_GLOBAL_USED_FLASH_SIZE" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_UBOOT_MONITOR_SIZE=%CONFIG_SPX_FEATURE_GLOBAL_UBOOT_MONITOR_SIZE" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_UBOOT_ENV_START=%CONFIG_SPX_FEATURE_GLOBAL_UBOOT_ENV_START" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_UBOOT_ENV_SIZE=%CONFIG_SPX_FEATURE_GLOBAL_UBOOT_ENV_SIZE" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_MEMORY_START=%CONFIG_SPX_FEATURE_GLOBAL_MEMORY_START" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_MEMORY_SIZE=%CONFIG_SPX_FEATURE_GLOBAL_MEMORY_SIZE" >>  %IMAGE_TREE/etc/core_macros
EXECUTE echo "CONFIG_SPX_FEATURE_GLOBAL_NIC_COUNT=%CONFIG_SPX_FEATURE_GLOBAL_NIC_COUNT" >>  %IMAGE_TREE/etc/core_macros
debug-install:
